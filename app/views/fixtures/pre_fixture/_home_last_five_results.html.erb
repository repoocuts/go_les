<% team.played_home_fixtures.reverse.each do |match| %>
	<ul class="max-w-md space-y-1 list-none list-inside">
		<li class="list-group-item text-sm">
			<%= link_to "#{match.home_score} - #{match.away_score} #{match.away_team_name}(GW#{match.game_week})", country_league_season_fixture_path(match.league.country_id, match.league, match.season, match) %>
		</li>
	</ul>
<% end %>

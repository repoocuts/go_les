<div>
  <% if team.top_scorer %>
    <%= link_to "#{team.top_scorer[1]} - #{team.top_scorer[0]}", player_path(team.top_scorer[2]) %>
  <% else %>
    <p>No goals</p>
  <% end %>
</div>
<div></div>
<div>
  <% if team.most_booked_player %>
    <%= link_to "#{team.most_booked_player.get_player_name} - #{team.most_booked_player.season_yellows}", player_path(team.most_booked_player.player) %>
  <% else %>
    <p>No cards</p>
  <% end %>
</div>

<div id="<%= dom_id corner %>">
  <% if action_name != "show" %>
    <%= link_to "Show this corner", corner, class: "rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%= link_to 'Edit this corner', edit_corner_path(corner), class: "rounded-lg py-3 ml-2 px-5 bg-gray-100 inline-block font-medium" %>
    <hr class="mt-6">
  <% end %>
</div>
